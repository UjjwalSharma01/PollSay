{
    "version": 2,
    "builds": [
      { 
        "src": "public/**", 
        "use": "@vercel/static",
        "config": {
          "buildCommand": "node utils/generate-env-config.js"
        }
      }
    ],
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "/public/$1" 
      }
    ],
    "env": {
      "SUPABASE_URL": "@supabase_url",
      "SUPABASE_ANON_KEY": "@supabase_anon_key"
    }
  }